{
  "name": "vlow",
  "version": "2.0.0",
  "description": "A simple library for unidirectional dataflow architecture inspired by Reflux",
  "main": "lib/index.js",
  "scripts": {
    "test": " ./node_modules/.bin/mocha --require @babel/register",
    "build:dev": "./node_modules/.bin/webpack -d --progress --profile --colors",
    "build:prod": "NODE_ENV='production' ./node_modules/.bin/webpack -p --progress --profile --colors",
    "babel": "npx babel src --out-dir lib && cp src/index.d.ts lib/ && echo \">WARNING: This \\`lib\\` folder is generated with \\`npm run babel\\` and should not be modified!\" > lib/README.md "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transceptor-technology/vlow.git"
  },
  "keywords": [
    "Reflux",
    "React",
    "Flux"
  ],
  "author": "Jeroen van der Heijden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/transceptor-technology/vlow/issues"
  },
  "homepage": "https://github.com/transceptor-technology/vlow#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "eslint": "^5.5.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "react": "^16.4.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  }
}
