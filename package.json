    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.2",
    "mocha": "^9.1.1",
    "react": "^17.0.2",
    "terser": "^5.8.0",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0"
